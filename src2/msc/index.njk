<script>
function myFunction() {
    var input, filter, ul, li, a, i, txtValue;
    input = document.getElementById("myInput");
    filter = input.value.toUpperCase();
    link = "http://pages.demon.live/msc/"+filter.substring(0,2)+"/"+filter[2]+"/"+filter.substring(3,5);
    console.log(link);
    window.location.href = link;
    return false;
}
</script>